<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Burger" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="android:statusBarColor">@color/aquaos</item>
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Theme.Burger" parent="Base.Theme.Burger" />

    <style name="BottomNavigation">

        <item name="android:background">#E4000000</item>
        <item name="itemIconTint">@color/aqua</item>
        <item name="itemTextColor">@color/aqua</item>

    </style>

    <style name="Theme.Burger.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Burger.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Burger.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>